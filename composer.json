{
    "name": "zeichen32/gitlabapibundle",
    "description": "Symfony2 Bundle to include the gitlab api and the gitlab issue tracker in your app.",
    "type": "symfony-bundle",
    "keywords": ["gitlab", "issue", "issuetracker", "api"],
    "homepage": "https://github.com/Zeichen32/GitLabApiBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jens Averkamp",
            "email": "j.averkamp@itm-systems.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": ">=2.1,<2.4-dev",
        "m4tthumphrey/php-gitlab-api": "*"
    },
    "autoload": {
        "psr-0": { "Zeichen32\\GitLabApiBundle": "" }
    },
    "target-dir": "Zeichen32/GitLabApiBundle",
    "minimum-stability": "dev"
}
